<?xml version="1.0" encoding="UTF-8"?>

<?import com.jfoenix.controls.JFXDrawer?>
<?import com.jfoenix.controls.JFXHamburger?>
<?import javafx.scene.Cursor?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.StackPane?>
<?import javafx.scene.shape.Circle?>

<AnchorPane fx:id="root" onKeyPressed="#keyPressed"
	xmlns="http://javafx.com/javafx/11.0.1"
	xmlns:fx="http://javafx.com/fxml/1">
	<children>
		<AnchorPane fx:id="overlay"
			style="-fx-background-color: rgba(255, 255, 255, 0.15);"
			AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0"
			AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0" />
		<AnchorPane fx:id="anchorPane" layoutX="330.0"
			maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308"
			minHeight="-Infinity" minWidth="-Infinity"
			style="-fx-background-color: #1F1F1F;" AnchorPane.bottomAnchor="0.0"
			AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0"
			AnchorPane.topAnchor="0.0">
			<children>
				<AnchorPane fx:id="loader"
					style="-fx-background-color: rgba(255,255,255,0.3);"
					AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0"
					AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
					<children>
						<HBox alignment="CENTER" maxHeight="1.7976931348623157E308"
							maxWidth="1.7976931348623157E308" AnchorPane.bottomAnchor="0.0"
							AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0"
							AnchorPane.topAnchor="0.0">
							<children>
								<StackPane maxHeight="250.0" maxWidth="250.0"
									prefHeight="250.0" prefWidth="250.0"
									style="-fx-background-color: rgba(255,255,255,0.05); -fx-background-radius: 50%;">
									<children>
										<Circle fx:id="c1" fill="TRANSPARENT" layoutX="110.0"
											layoutY="86.0" radius="100.0" stroke="#14a1ff"
											strokeType="INSIDE" strokeWidth="9.0"
											style="-fx-stroke-dash-array: 50;" />
										<Circle fx:id="c2" fill="TRANSPARENT" layoutX="110.0"
											layoutY="110.0" radius="75.0" stroke="#2a17dd"
											strokeType="INSIDE" strokeWidth="7.0"
											style="-fx-stroke-dash-array: 25;" />
										<Circle fx:id="c3" fill="TRANSPARENT" layoutX="110.0"
											layoutY="110.0" radius="50.0" stroke="#14a1ff"
											strokeType="INSIDE" strokeWidth="5.0"
											style="-fx-stroke-dash-array: 19;" />
									</children>
								</StackPane>
							</children>
						</HBox>
					</children>
				</AnchorPane>
				<AnchorPane fx:id="modelView"
					style="-fx-background-color: #1f1f1f;"
					AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0"
					AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
					<children>
						<AnchorPane fx:id="patientView" layoutX="10.0"
							layoutY="10.0" prefHeight="200.0" prefWidth="200.0"
							AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0"
							AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0" />
						<AnchorPane fx:id="doctorView" layoutX="20.0"
							layoutY="20.0" prefHeight="200.0" prefWidth="200.0"
							AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0"
							AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0" />
						<AnchorPane fx:id="appointmentView" layoutX="30.0"
							layoutY="30.0" prefHeight="200.0" prefWidth="200.0"
							AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0"
							AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0" />
						<AnchorPane fx:id="homeView" prefHeight="200.0"
							prefWidth="200.0" style="-fx-background-color: #1f1f1f;"
							AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0"
							AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0" />
					</children>
				</AnchorPane>
				<AnchorPane fx:id="drawerOverlay"
					style="-fx-background-color: rgba(255, 255, 255, 0.15);" />
				<JFXDrawer fx:id="drawer" defaultDrawerSize="300.0"
					maxHeight="-Infinity" maxWidth="-Infinity" prefHeight="1080.0"
					prefWidth="300.0" AnchorPane.bottomAnchor="0.0"
					AnchorPane.leftAnchor="0.0" AnchorPane.topAnchor="0.0" />
				<JFXHamburger fx:id="hamburger"
					layoutX="135.1999969482422" layoutY="530.4000244140625"
					maxHeight="-Infinity" maxWidth="-Infinity"
					onMouseClicked="#hamburgerToggle" AnchorPane.leftAnchor="30.0"
					AnchorPane.topAnchor="30.0">
					<cursor>
						<Cursor fx:constant="HAND" />
					</cursor>
				</JFXHamburger>
			</children>
		</AnchorPane>
	</children>
</AnchorPane>
